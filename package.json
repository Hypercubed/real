{
  "name": "real",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/real.umd.js",
  "module": "dist/real.es5.js",
  "typings": "dist/types/real.d.ts",
  "files": [
    "dist"
  ],
  "author": "Hypercubed <hypercubed@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "test": "ava",
    "test:watch": "ava --watch --verbose --only",
    "test:prod": "npm run lint && ava --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "./test/**/*.test.ts"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^10.11.0",
    "ava": "^2.4.0",
    "coveralls": "^3.0.2",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@hypercubed/dynamo": "^1.0.2"
  }
}
